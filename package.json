{
  "name": "inpsyq-landing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "seed:prod": "npx -y tsx scripts/seed_prod.ts",
    "seed:dev": "npx tsx scripts/seed_dev.ts",
    "bootstrap:dev": "npx tsx scripts/bootstrap_dev.ts",
    "verify:briefing": "npx tsx scripts/verify_briefing.ts",
    "verify:llm_session": "npx tsx scripts/verify_llm_session.ts",
    "verify:measurement_layer": "npx tsx scripts/verify_measurement_layer.ts",
    "verify:full": "npx tsx scripts/verify_model_integrity_full.ts",
    "itembank:build": "npx tsx scripts/build_item_bank.ts",
    "itembank:stats": "npx tsx scripts/stats_item_bank.ts",
    "itembank:lint": "npx tsx scripts/lint_item_bank.ts",
    "verify:voice_layer": "npx tsx scripts/verify_voice_layer.ts",
    "verify:prod_parity": "npx tsx scripts/verify_prod_parity.ts",
    "verify:prod_session": "npx tsx scripts/verify_prod_session.ts",
    "verify:parity": "npx tsx scripts/verify_parity.ts",
    "sim:dev": "npx tsx scripts/simulate_sessions_dev.ts",
    "sim:dev:small": "npx tsx scripts/simulate_sessions_dev.ts --small",
    "agg:dev": "npx tsx scripts/rebuild_aggregates_dev.ts",
    "sim:dev:ready": "npm run seed:dev && npm run sim:dev && npm run agg:dev && npm run verify:sim && npm run verify:dashboard",
    "verify:sim": "npx tsx scripts/verify_simulation_ready.ts",
    "verify:dashboard": "npx tsx scripts/verify_dashboard_ready.ts",
    "verify:temporal": "npx tsx scripts/verify_temporal_sim.ts",
    "verify:dashboard_ui": "npx tsx scripts/verify_dashboard_ui.ts",
    "ids": "node -e \"fetch('http://localhost:3001/api/internal/ids').then(r=>r.json()).then(console.log)\"",
    "ids:prod": "node -e \"fetch('https://www.inpsyq.com/api/internal/ids',{headers:{'x-inpsyq-admin-secret':process.env.INTERNAL_ADMIN_SECRET}}).then(r=>r.json()).then(console.log)\""
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "@types/pg": "^8.16.0",
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.300.0",
    "next": "14.1.0",
    "openai": "^6.10.0",
    "pg": "^8.16.3",
    "react": "^18",
    "react-dom": "^18",
    "recharts": "^3.5.1",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}